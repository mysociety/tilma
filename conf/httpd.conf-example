# Apache configuration for TilMa

AddHandler fcgid-script .fcgi

<Location ~ "\.png$">
    ExpiresActive On
    ExpiresDefault "access plus 1 week"
</Location>

# Prevent errors from favicon.ico
RewriteEngine on
RewriteRule /favicon.ico / [F]

RewriteRule ^/(osm|vmd|mapumental-map|mapumental-names|mapumental-map-print|mapumental-names-print)/(.*)  /tilecache.fcgi/1.0.0/$1/$2

